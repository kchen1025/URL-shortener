{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "URL shortener",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "debug": "nodemon --watch src --exec node -r ts-node/register --inspect=9228 src/index.ts",  
    "build": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "dev:client": "npm run dev --prefix=src-client",
    "dev-all": "concurrently \"npm run dev\" \"npm run dev --prefix src-client\"",
    "build-all": "npm run build && npm run build --prefix src-client",
    "lint-all": "tslint -c tslint.json -p tsconfig.json && npm run lint --prefix src-client",
    "db": "ts-node ./src/utils/runQuery.ts",
    "test": "jest"
  },
  "author": "kchen1025",
  "license": "ISC",
  "dependencies": {
    "@types/joi": "^14.3.4",
    "@types/node": "^14.0.27",
    "@types/shortid": "0.0.29",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-session": "^1.17.1",
    "http-status-codes": "^1.4.0",
    "joi": "^14.3.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.3",
    "pg": "^8.2.1",
    "shortid": "^2.2.15",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/jest": "^26.0.4",
    "@types/morgan": "^1.9.1",
    "@types/passport": "^1.0.4",
    "@types/passport-auth0": "^1.0.4",
    "@types/pg": "^7.14.3",
    "@types/supertest": "^2.0.10",
    "concurrently": "^4.1.0",
    "jest": "^26.1.0",
    "nodemon": "^1.18.9",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  }
}
